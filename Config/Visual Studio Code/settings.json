"vscord.status.image.large.editing.text": "Editing {LANG} file ({file_extension})",
"vscord.status.image.large.notInFile.text": "Not in a file",
"vscord.status.image.large.viewing.text": "Viewing {LANG} file ({file_extension})",
"vscord.status.image.small.idle.text": "Sleepin...",
"vscord.status.problems.enabled": false,
"vscord.status.state.text.debugging": "Debugging: {file_name}",
"vscord.status.state.text.editing": "Editing: {file_name}",
"vscord.status.state.text.idle": "Idle",
"vscord.status.state.text.notInFile": "Not in a file",
"vscord.status.state.text.noWorkspaceFound": "",
"vscord.status.state.text.viewing": "Viewing: {file_name}",

"window.autoDetectColorScheme": true,
"window.commandCenter": false,
"window.density.editorTabHeight": "compact",
"window.title": "${rootName}${separator}${profileName}",

"workbench.activityBar.location": "bottom",
"workbench.colorTheme": "Winter is Coming (Dark Blue)",
"workbench.editor.empty.hint": "hidden",
