"discord.lowerDetailsIdling": "üåô Idling",
"discord.lowerDetailsNoWorkspaceFound": "‚ùì No workspace",
"discord.smallImage": "I use {app_name}!",

"editor.accessibilitySupport": "off",
"editor.detectIndentation": false,
"editor.fontFamily": "'JetBrains Mono', Consolas, 'Courier New', monospace",
"editor.fontLigatures": true,
"editor.fontSize": 13,
"editor.guides.bracketPairs": true,
"editor.minimap.enabled": false,
"editor.mouseWheelZoom": true,
"editor.multiCursorModifier": "ctrlCmd",
"editor.renderControlCharacters": false,
"editor.renderWhitespace": "all",
"editor.smoothScrolling": true,
"editor.stickyScroll.enabled": true,
"editor.unicodeHighlight.allowedLocales": {
    "ru": true
},
"editor.wordWrap": "on",

"explorer.autoReveal": false,
"explorer.copyRelativePathSeparator": "/",

"extensions.ignoreRecommendations": true,

"files.associations": {
    "*.edx": "markdown",
    "*.2do": "markdown"
},
"files.autoSave": "afterDelay",
"files.autoSaveDelay": 10000,
"files.enableTrash": false,

"fountain.general.previewTheme": "paper",
"fountain.pdf.printNotes": true,
"fountain.pdf.printSections": true,
"fountain.pdf.printSynopsis": true,
"fountain.pdf.textCONTD": "(CONT‚ÄôD)",

"git.autofetch": true,
"git.confirmSync": false,
"git.enableSmartCommit": true,

"l13Diff.confirmCopy": false,
"l13Diff.confirmDelete": false,
"l13Diff.enablePreview": true,
"l13Projects.confirmDeleteProject": false,

"liveServer.settings.AdvanceCustomBrowserCmdLine": "C:\\Program Files\\BraveSoftware\\Brave-Browser\\Application\\brave.exe --guest",
"liveServer.settings.donotShowInfoMsg": true,
"liveServer.settings.donotVerifyTags": true,
"liveServer.settings.useWebExt": true,

"Live-Workspace.username": "{{CHANGE}}",

"markdown-preview-enhanced.scrollSync": false,

"material-icon-theme.files.associations": {
    "2do.md":       "todo",
    "*.2do":        "todo",
    "*.edx":        "supabase",
    "*.fountain":   "fortran",
    "*.issue":      "ionic",
    "*.lnk":        "url"
},
"material-icon-theme.folders.associations": {
    "2do":          "tasks",
    "gears":        "config",
    "html_include": "include",
    ".obsidian":    "config"
},

"outline.collapseItems": "alwaysCollapse",

"remotefs.remote": {
    "{{CHANGE}}": {
        "scheme": "ftp",
        "host": "{{CHANGE}}",
        "username": "{{CHANGE}}",
        "password": "{{CHANGE}}",
        "rootPath": "/"
    }
},

"scoped-sort.defaultArgs": {
    "prompt":       "--case-insensitive",
    "no-prompt":    "--case-insensitive"
},

"search.searchEditor.defaultNumberOfContextLines": 0,

"security.workspace.trust.untrustedFiles": "open",

"statsBar.location": "Right",
"statsBar.modules": [
    "cpuLoad",
    "networkSpeed"
],
"statsBar.networkSpeed.format": "$(arrow-up) ${up} $(arrow-down) ${down}",

"symbols.hidesExplorerArrows": false,

"telemetry.telemetryLevel": "off",

"terminal.integrated.defaultProfile.windows": "Command Prompt",
"terminal.integrated.smoothScrolling": true,

"todohighlight.defaultStyle": {
    "border": "2px dotted #ff9400",
    "color": "black",
    "backgroundColor": "#ffbf00"
},
"todohighlight.keywords": ["BUG","HACK","FIXME","TODO","XXX","[ ]","[x]","2DO","TEMP","CONTINUE HERE","2BREMOVED"],

"TodoParser": {
    "markers": ["BUG","HACK","FIXME","TODO","XXX","[ ]","[x]","2DO","TEMP","CONTINUE HERE","2BREMOVED"]
},

"todo-tree.filtering.includeHiddenFiles": true,
"todo-tree.general.showActivityBarBadge": true,
"todo-tree.general.statusBar": "current file",
"todo-tree.general.tags": [
    "BUG","HACK","FIXME","TODO","XXX","[ ]","[x]","2DO","TEMP","CONTINUE HERE","2BREMOVED","2do","TBA"
],
"todo-tree.highlights.customHighlight": {
    "BUG":          {"icon": "bug"},
    "HACK":         {"icon": "tools"},
    "FIXME":        {"icon": "flame"},
    "XXX":          {"icon": "x"},
    "[ ]":          {"icon": "issue-draft"},
    "[x]":          {"icon": "issue-closed"}
},
"todo-tree.highlights.enabled": true,
"todo-tree.regex.regex": "(//|#|<!--|;|/\\*|\\[\\[|^|^[ \\t]*(-|\\d+.))\\s*($TAGS)",
"todo-tree.regex.subTagRegex": "\\]\\]",
"todo-tree.tree.buttons.export": true,
"todo-tree.tree.buttons.groupBySubTag": true,
"todo-tree.tree.buttons.scanMode": true,
"todo-tree.tree.showCountsInTree": true,
"todo-tree.tree.showScanModeButton": false,

"update.mode": "start",
"update.showReleaseNotes": false,

"vscode-edge-devtools.mirrorEdits": true,

"vscode-json-editor.theme": "dark",

"VSCodeNotion.accessToken": "{{CHANGE}}",

"vscord.status.buttons.button1.git.active.label": "View repository",
"vscord.status.buttons.button1.git.idle.label": "View repository",
