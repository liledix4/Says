{
"[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
},

"csv-edit.readOption_hasHeader": "true",

"diffEditor.ignoreTrimWhitespace": false,

"discord.detailsDebugging": "{workspace}",
"discord.detailsEditing": "{workspace}",
"discord.detailsIdling": "üåô Idling",
"discord.idleTimeout": 4444,
"discord.largeImage": "‚úèÔ∏è Editing {LANG} file",
"discord.largeImageIdling": "üåô Idling",
"discord.lowerDetailsDebugging": "üêû {file_name}",
